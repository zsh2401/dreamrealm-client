        Gen1=true
        # 宝可梦图鉴 152-251
        Gen2=true
        # 宝可梦图鉴 252-386
        Gen3=true
        # 宝可梦图鉴 387-493
        Gen4=true
        # 宝可梦图鉴 494-649
        Gen5=true
