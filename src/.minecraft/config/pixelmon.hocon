# 设置AFK玩家的自动对战。
AFKHandler {
    # 在对战中若玩家AFK则用自动对战代码托管战斗。
    afkActivateSeconds=90
    # 自动对战系统每回合自动选取几个行动。
    afkHandlerTurnSeconds=15
    # 如果有玩家在对战中AFK自动对战，则在指定时间内进行回合。
    enableAFKHandler=false
}
# 设置繁殖状态。
Breeding {
    # 允许放置牧场方块，这是用于繁殖宝可梦的必要物品。
    allowBreeding=true
    # 允许两只百变怪生蛋随机产生宝可梦蛋。
    allowDittoDittoBreeding=true
    # 允许牧场拓展。
    allowRanchExpansion=true
    # 允许两只百变怪生蛋随机产生神兽蛋。
    allowRandomBreedingEggsToBeLegendary=false
    # 经过每个繁殖阶段需要多少Tick。
    breedingTicks=18000
    # 设置玩家在所有维度世界里可以获得的宝可梦总数量 (0为不限制) 
    maxCumulativePokemonInRanch=0
    # 繁殖时需要几个感情等级(1-5)。
    numBreedingStages=5
    # 设置宝可梦蛋一个周期需要的步数。数字越小，孵化蛋越快。
    stepsPerEggCycle=255
    # 根据育种不同来控制繁殖速度。若设置为false则繁殖速度为默认的1.0倍。
    useBreedingEnvironment=true
}
# 选项。
Elevator {
    # 设置升降机的最大上下范围。
    elevatorSearchRange=10
}
# 用于设置是否启用外部文件的设置
ExternalFiles {
    # 设置使用此文件夹下的 ./pixelmon/spawning/ 哪一个生成规则文件。此选项适用于新的宝可梦生成机制。
    spawnSetFolder=default
    # 是否针对更好的宝可梦生成机制使用位于 config/pixelmon 路径下的文件
    useBetterSpawnerConfig=false
    # 从外部位置 ("pixelmon/drops") 加载用于 掉落物品 的配置文件，如果此项设为true并且"pixelmon/drops"文件夹不存在，Pixelmon则会生成这个文件夹并转储现有的配置。
    useExternalJSONFilesDrops=false
    # 从外部位置 ("pixelmon/moves") 加载用于 招式 的配置文件，如果此项设为true并且"pixelmon/moves"文件夹不存在，Pixelmon则会生成这个文件夹并转储现有的配置。
    useExternalJSONFilesMoves=false
    # 从外部位置 ("pixelmon/npcs" ) 加载用于 NPC 的配置文件，如果此项设为true并且"pixelmon/npcs"文件夹不存在，Pixelmon则会生成这个文件夹并转储现有的配置。
    useExternalJSONFilesNPCs=false
    # Load external configuration files used for quests from the "pixelmon/quests" folder. If set to true and the "pixelmon/quests" folder does not exist, Pixelmon will create the folder and dump the existing files there.
    useExternalJSONFilesQuests=false
    # 从外部位置 ("pixelmon/rules") 加载用于 战斗规则 的配置文件，如果此项设为true并且"pixelmon/rules"文件夹不存在，Pixelmon则会生成这个文件夹并转储现有的配置。
    useExternalJSONFilesRules=false
    # 从外部位置 ("pixelmon/spawning") 加载用于 生成器 的配置文件，如果此项设为true并且"pixelmon/spawning"文件夹不存在，Pixelmon则会生成这个文件夹并转储现有的配置。
    useExternalJSONFilesSpawning=false
    # 从外部位置 ("pixelmon/stats") 加载用于 状态 的配置文件，如果此项设为true并且"pixelmon/stats"文件夹不存在，Pixelmon则会生成这个文件夹并转储现有的配置。
    useExternalJSONFilesStats=false
    # 从外部位置 ("pixelmon/structures") 加载用于 建筑物 的配置文件，如果此项设为true并且"pixelmon/structures"文件夹不存在，Pixelmon则会生成这个文件夹并转储现有的配置。
    useExternalJSONFilesStructures=false
}
# 设置对战外招式。
ExternalMoves {
    # 是否允许玩家使用破坏性对战外招式。 (包括:居合斩/挖洞/爆炸/火光/闪电/碎岩)
    allowDestructiveExternalMoves=true
    # 是否允许玩家使用对战外招式如碎岩。
    allowExternalMoves=true
}
# 世界常规设置。
General {
    # 允许护符金币在对战后给予2倍的宝可梦币对战奖励
    allowAmuletCoin=true
    # 允许铁砧在玩家操作后自动将玩家背包内的初始操作物品替换成已完成的物品。
    allowAnvilAutoreloading=false
    # 是否允许玩家在非对战情况下丢出精灵球。如果设置为false则只能在对战中扔精灵球。
    allowCaptureOutsideBattle=true
    # 服务器端设置，是否允许玩家使用宝可梦雕刻工具
    allowChisels=true
    # 允许是否在使用欢乐时光招式后翻倍宝可梦币奖励
    allowHappyHour=true
    # 允许聚宝功在对战后给予额外的宝可梦币奖励
    allowPayDay=true
    # 是否允许玩家种植树果。
    allowPlanting=true
    # 当使用服务器端时，此选项设置其他玩家是否可以使用宝可梦编辑器。当在客户端使用时，是否允许其他玩家修改另一人的宝可梦编辑器。
    allowPokemonEditors=true
    # 允许玩家给宝可梦命名(这不是服务器设置，是客户端设置)。
    allowPokemonNicknames=true
    # 是否允许玩家之间的对战中使宝可梦获得经验。当设置为false时无经验。
    allowPvPExperience=true
    # 无论是通过/pokegive还是其他任何pokespec生成的宝可梦都有可能包含传说中的宝可梦。
    allowRandomPokemonToBeLegendary=true
    # 当完成全宝可梦图鉴时解锁闪耀护符
    allowShinyCharmFromPokedex=true
    # 是否允许招式教学机重复使用。
    allowTMReuse=false
    # 是否允许和NPC的对战中使宝可梦获得经验。
    allowTrainerExperience=true
    # 是否允许苦力怕,骷髅怪,等怪物生成。危险呢-它会在您对战的时候杀死您。
    allowVanillaMobs=false
    # 不管怎样，玩家总会自动获得超级环。
    alwaysHaveMegaRing=false
    # 设置平均多少个tick后引虫香水的效果将失效
    averageLureExpiryTicks=17920
    # 当玩家战胜野生宝可梦时给予照片
    awardPhotos=false
    # 对战中BOSS的AI类型。
    battleAIBoss=2
    # 对战中NPC训练师的AI类型。
    battleAITrainer=3
    # 对战中野怪的AI类型。
    battleAIWild=1
    # 当玩家睡在床上时治愈玩家的宝可梦。
    bedsHealPokemon=true
    # 果树的成长速度的倍数，请保持在0.1至1000之间。
    berryTreeGrowthMultiplier=1
    # 无论在什么情况下，在对战外的野生宝可梦都可以受到物理攻击。
    canPokemonBeHit=false
    # 设定使用豪华球收服大牙狸之后，将变成特殊大牙狸的概率 (-1 为禁用)
    chanceToGetSpecialBidoof=30
    # 是否允许玩家使用梦幻来克隆超梦。
    cloningMachineEnabled=true
    # 设置玩家的PC的最大可用页数 (上限为256页) 
    computerBoxes=30
    # 如果玩家背包或PC被强制保存，则自动保存世界 (如果为false，则你需要通过/saveall指令来保存内容，或自行承担数据丢失的问题) 
    dataSaveOnWorldSave=true
    # 是否在掉落界面显示删除全部物品按钮.
    deleteUnwantedDrops=false
    # 设置宝可梦实体会消失在距离最近的玩家身边的最短距离。
    despawnRadius=80
    # 确定是否禁止一切非玩家的宝可梦移动
    disabledNonPlayerPixelmonMovement=false
    # 是否允许野生宝可梦主动对玩家发起战斗
    enableWildAggression=true
    # 如果启用，玩家将可以通过投掷宝可梦到玩家身上进行对战，而不是玩家的宝可梦。
    engagePlayerByPokeBall=true
    # 宝可梦对战获得的经验的倍数。
    expModifier=1
    # 当一个对战于某种未知情况下没有正常结束时强行结束的结果。0-由剩余宝可梦的HP决定胜者。1-认定这为一个平局。2-认定这为异常结束。
    forceEndBattleResult=0
    # 调整升级对体型的变化影响(0-2)。1为普通。
    growthScaleModifier=1.0
    # 设置亚克诺姆、艾姆利多、由克希可以将红宝石进行附魔的次数。
    lakeTrioMaxEnchants=3
    # 宝可梦最大能达到的等级。
    maxLevel=100
    # 每个区块内允许种植树果的最大数目，适用于树果和果实。
    maximumPlants=32
    # 设置招式教学师所拥有的招式数量
    movesPerTutor=20
    # 允许玩家重复获得相同的精灵照片。
    multiplePhotosOfSamePokemon=true
    # 若设置为true，则必须要管理员才可以使用NPC编辑器。若为false，则需要创造模式
    npcEditorOpOnly=true
    # 设置矿物感应招式的矿物颜色。格式为"方块;颜色(hex格式);图案号码
    oreColors=[
        "minecraft:iron_ore;E2C0AA;0",
        "minecraft:gold_ore;FCEE4B;0",
        "minecraft:redstone_ore;FF0000;0",
        "minecraft:emerald_ore;17DD62;1",
        "minecraft:diamond_ore;65F5E3;0",
        "minecraft:lapis_ore;1855BD;2",
        "minecraft:quartz_ore;EAE5DE;3",
        "pixelmon:amethyst_ore;9543A9;4",
        "pixelmon:sun_stone_ore;E57742;5",
        "pixelmon:silicon_ore;E2DFEE;0",
        "pixelmon:sapphire_ore;4A73E7;1",
        "pixelmon:ruby_ore;DD1C33;1",
        "pixelmon:fossil;443D32;6",
        "pixelmon:crystal_ore;AFEEEE;4",
        "pixelmon:bauxite_ore;D1B584;0",
        "pixelmon:dawn_dusk_ore;000000;7",
        "pixelmon:water_stone_ore;5D719F;8",
        "pixelmon:thunder_stone_ore;F0FF01;9",
        "pixelmon:leaf_stone_ore;7EA167;10",
        "pixelmon:fire_stone_ore;CC7132;11"
    ]
    # 设置聚宝功给予的额外宝可梦币倍数
    payDayMultiplier=5.0
    # 捡拾特性在战斗后获得道具的几率 (1/几率). 0 为禁用此功能。
    pickupRate=10
    # 是否让宝可梦死亡后掉落物品。
    pokemonDropsEnabled=true
    # 如果启用此设置则生成更详细的错误报告到server.log。
    printErrors=true
    # 如果启用，对战中损失的携带道具将于对战结束后返还给相应宝可梦。
    returnHeldItems=true
    # 是否允许祭坛多次使用。
    reusableBirdShrines=false
    # 是否允许神鸟神社随机生成。
    spawnBirdShrines=true
    # pixelmon.config.spawnCelebiShrines.comment
    spawnCelebiShrines=true
    # 允许随机生成建筑如:宝可梦中心，神社祭坛和其他建筑物，true为生成。
    spawnStructures=true
    # 如果设置为true则必须为OP才能生成宝可梦，如果设置为false则必须为创造模式。
    spawnersOpOnly=true
    # 请您选择提供/不提供当一个玩家进入游戏时的首发伙伴(通过Pixelmon的进入页面)。
    starterOnJoin=true
    # 将之前的时间同步为系统时间(以秒为单位)。
    systemTimeSyncInterval=30
    # 设置交换者一次性使用(false)或设置交换者可多次使用(true)。
    tradersReusable=false
    # 是否在击败一个常规宝可梦时显示掉落界面。
    useDropGUI=true
    # 使用系统时间作为树果成长的依据。这意味着即使游戏并未运行，它们仍将会随着真实世界时间流逝而成长
    useSystemTimeForBerries=false
    # 是否让游戏使用现实时间(根据服务端的系统时间)。
    useSystemTimeForWorldTime=false
    # pixelmon.config.verbose.comment
    verbose=false
    # 当保存世界时保存Pixelmon实体到文件中(如果为true，大大增加了保存文件的大小和保存时间)。需要变形mod支持。
    writeEntitiesToWorld=false
}
# 图像设置。
Graphics {
    # 在宝可梦头上显示其血量方块。
    drawHealthBars=false
    # 设置客户端可以看见宝可梦名称和等级标签的距离 (仅客户端)。1=默认, 2=较远, 3=远
    namePlateRange=1
    # 如果设置为 true，则仅向参与或观看战斗的玩家显示攻击效果。
    onlyShowAttackEffectsToBattlers=true
    # 是否设置玩家对战中可调整镜头方向(设置镜头基于球面的大小)
    playerControlCamera=true
    # 如果玩家并未在战斗，则向该玩家显示其他战斗动画的显示范围 (以方块为单位) 。
    rangeToDisplayAttackAnimations=40
    # 通常以64个方块为距离的渲染。
    renderDistanceWeight=2.0
    # 设置户外是否显示野生宝可梦的等级
    renderWildLevels=true
    # 在游戏中使用提高了30%体型的宝可梦。
    scalePokemonModels=true
    # 在画面中显示当前宝可梦攻击的目标。
    showCurrentAttackTarget=true
    # 是否在宝可梦名牌上显示其名称。
    showWildNames=true
    # pixelmon.config.smoothAnimations.comment
    smoothAnimations=false
    # 是否设置对战镜头改为外部镜头设置。
    useBattleCamera=true
    # 使用平滑纹理的精灵球模型。
    useSmoothShadingOnPokeBalls=true
    # 向SMD模型进行动画插值补帧。将会造成游戏性能下降。
    useSmoothShadingOnPokemon=true
}
# 用于设置在行走时需要如何治疗宝可梦
Healing {
    # 每走一步都有概率被动的恢复宝可梦的状态
    chanceToHealStatusPassively=0.004999999888241291
    # 每走一步都有概率被动的恢复濒死状态的宝可梦
    chanceToRevivePassively=0.0010000000474974513
    # 被动治疗可以到达的最大百分比
    passiveHealingMaxHealthPercentage=0.25
    # 设置在行走多少步后为宝可梦恢复1点HP
    stepsToHealHealthPassively=75
    # 是否随着时间对宝可梦栏内的宝可梦进行治疗
    usePassiveHealer=true
}
# pixelmon.config.outsideeffects.comment
OutsideEffects {
    # 中毒效果每次能给宝可梦施加的最大伤害
    poisonMaxDamage=5
    # 效果 (如中毒) 每次结算的步数间隔
    stepsToApplyEffects=15
    # 是否启用对战外回复效果
    useOutsideEffects=true
}
# 设置宝可梦实用工具的功能。
PixelUtilities {
    # 指定宝可梦礼品盒出现的坐标。
    eventCoords=[
        notConfigured
    ]
    # 允许/禁止神兽被放入宝可梦礼品盒。
    eventHasLegendaries=false
    # 每只宝可梦有1/10的几率成为异色的。
    eventHasShinies=false
    # 设置宝可梦礼品盒中最多的宝可梦数。
    eventMaxPokemon=1
    # 是否允许宝可梦礼品盒生成。
    eventPokeGiftLoad=false
    # 礼品盒中出现异色宝可梦的几率(1/x)。
    eventShinyRate=10
    # 选定一个能打开宝可梦礼品盒的日期(先日期接着月)
    eventTime="D/M"
    # 是否允许使用宝可梦礼品盒(包括放置和使用，放置无用礼品盒)。
    pokeGiftHaveEvents=true
    # 是否允许玩家在宝可梦礼物盒中得到宝可梦。
    pokeGiftReusable=false
    # 根据玩家队伍决定宝可梦草丛中的宝可梦的水平？
    scaleGrassBattles=false
}
# 设置宝可梦奖励箱的生成和状态。
PokeLoot {
    # 允许隐藏洞穴随机生成。
    spawnGrotto=true
    # 允许隐形的宝可梦奖励箱随机生成。
    spawnHidden=true
    # 默认生成模式 (FCFS=0, PL1D=1, PUD=2, TIMED=3). FCFS = 先到先得，领取后消失。PL1D = 每个玩家给与一次。PUD = 每位玩家可以无限领取此奖励箱。 TIMED = 每位玩家可以在间隔时间内领取一次奖励箱的内容。
    spawnMode=0
    # 允许普通的宝可梦奖励箱随机生成。
    spawnNormal=true
    # 奖励箱生成率。0:64个区块里1个。1:32个区块里1个。2:18个区块里1个。3:10个区块里1个。
    spawnRate=1
    # 若设置为 TIMED，则需要设置玩家可以重新使用宝可梦奖励箱的时间 (小时)
    timedLootReuseHours=24
}
# 宝可病毒相关的设置
Pokerus {
    # 宝可病毒是否启用，这将会影响宝可梦生长、繁殖和生成
    pkrsEnabled=true
    # 告知玩家宝可梦是否感染宝可病毒
    pkrsInformPlayers=true
    # 宝可病毒的发病率 (1/x)。 (0 为不生成)
    pkrsSpawnRate=12288
    # 在一次对战后，病患宝可梦向周围宝可梦传播病毒。 (1/x) (-1 为不生效，0 为保持生效)
    pkrsSpreadRate=5
}
# Settings that alter how quests function.
Quests {
    # The maximum distance between elevators pairing with each other.
    questMaxRange=500
    # Whether to let NPCs be randomly assigned quests or not. This does not affect quests that have already been started.
    questRandomNPCs=true
}
# 用于控制乘骑宝可梦的设置
Riding {
    # 是否允许玩家骑乘宝可梦(服务器设置)。
    allowRiding=true
    # 当乘骑宝可梦时，宝可梦面向的方向将跟随玩家准星所指向的方向。
    enablePointToSteer=true
    # 即使宝可梦不可以飞行或者冲浪，但他们仍可以被乘骑并在地面上移动
    landMount=true
    # 只有可以学会飞行/冲浪秘传学习器的宝可梦才可以用于乘骑
    requireHM=false
    # 宝可梦乘骑速度倍数，请保持在0至1之间。
    ridingSpeedMultiplier=1
}
# 设置宝可梦生成多少。
Spawning {
